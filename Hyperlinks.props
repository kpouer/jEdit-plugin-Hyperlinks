# Hyperlinks jEdit Plugin
# Author: Matthieu Casanova

plugin.gatchan.jedit.hyperlinks.HyperlinksPlugin.activate=startup
plugin.gatchan.jedit.hyperlinks.HyperlinksPlugin.name=Hyperlinks
plugin.gatchan.jedit.hyperlinks.HyperlinksPlugin.author=Matthieu Casanova
plugin.gatchan.jedit.hyperlinks.HyperlinksPlugin.version=1.2.0
plugin.gatchan.jedit.hyperlinks.HyperlinksPlugin.docs=docs/Hyperlinks.html
plugin.gatchan.jedit.hyperlinks.HyperlinksPlugin.depend.0=jdk 1.6
plugin.gatchan.jedit.hyperlinks.HyperlinksPlugin.depend.1=jedit 04.05.99.00
plugin.gatchan.jedit.hyperlinks.HyperlinksPlugin.description=The hyperlink plugin will allow the textarea to have hyperlinks inside.
plugin.gatchan.jedit.hyperlinks.HyperlinksPlugin.usePluginHome=true

# Option pane
plugin.gatchan.jedit.hyperlinks.HyperlinksPlugin.option-group=hyperlink hyperlink.source

options.hyperlink.label=General
options.hyperlink.code=new gatchan.jedit.hyperlinks.HyperlinkOptionPane();


options.hyperlink.source.label=Sources
options.hyperlink.source.code=new gatchan.jedit.hyperlinks.HyperlinkSourceOptionPane();
options.hyperlink.source.source=Source

options.hyperlink.color.label=Color
options.hyperlink.color.value=#3333ff

hyperlink.defaultSource.label=Default source
hyperlink.defaultSource=None

mode.c.hyperlink.source=C:include
mode.c++.hyperlink.source=C:include
mode.objective-c.hyperlink.source=C:include

plugin.gatchan.jedit.hyperlinks.HyperlinksPlugin.menu=reload-hyperlinks-xml open-hyperlinks-xml
reload-hyperlinks-xml.label=Reload hyperlinks.xml
open-hyperlinks-xml.label=Open hyperlinks.xml